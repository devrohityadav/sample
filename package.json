  "main": "src/index.ts",
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.3",
    "express-validator": "^6.6.0",
    "multer": "^1.4.2",
    "pg": "^8.2.1",
    "uuid": "^8.2.0"
  },
  "scripts": {
    "build": "tsc",
    "test": "mocha",
    "dev": "nodemon --exec ts-node src/index.ts",
